---
{
	"layout": "core.hbs"
}
---
<div class="wb-sppe">
	<header>
		<div id="wb-bnr">
			<div class="container">
				<div class="row mrgn-tp-lg mrgn-bttm-lg">
					<div id="wb-spllg" class="col-md-8 col-md-offset-2">
						<img src="{{assets}}/../{{site.theme}}/assets/logo.svg" alt="{{{i18n "site-title"}}}" />
					</div>
				</div>
			</div>
		</div>
	</header>
	<main property="mainContentOfPage" typeof="WebPageElement" resource="#wb-main" class="container">
		<div class="row mrgn-tp-lg">
			<div class="col-md-12">
				{{>body}}
			</div>
		</div>
	</main>
</div>
